{
  "expo": {
    "name": "KÃ¤mppis App",
    "slug": "kamppis-app",
    "version": "1.0.0",
    "owner": "nat20",
    "orientation": "portrait",
    "icon": "./assets/images/kamppis-app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nat20.kamppisapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/kamppis-app-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.nat20.kamppisapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#000000",
      "barStyle": "light-content",
      "translucent": false
    },
    "backgroundColor": "#000000",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/kamppis-app-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos so you can upload them to your profile.",
          "cameraPermission": "The app needs access to your camera so you can take a profile picture."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "BACKEND_URL": "https://kamppis.hellmanstudios.fi",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7987255a-a681-455b-b17b-4692ce3d55f9"
      }
    }
  }
}
